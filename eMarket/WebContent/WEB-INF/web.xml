<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

  <display-name>eMarket</display-name>

     <servlet>
         <servlet-name>ModificarCatalogoServlet</servlet-name>
         <servlet-class>
             es.indra.formacion.pr.web.servlet.catalogo.ModificarCatalogoServlet
         </servlet-class>
     </servlet>

     <servlet-mapping>
         <servlet-name>ModificarCatalogoServlet</servlet-name>
         <url-pattern>/catalogo/ModificarCatalogo</url-pattern>
     </servlet-mapping>

     <servlet>
         <servlet-name>PrincipalCatalogoServlet</servlet-name>
         <servlet-class>
             es.indra.formacion.pr.web.servlet.catalogo.PrincipalCatalogoServlet
         </servlet-class>
     </servlet>

     <servlet-mapping>
         <servlet-name>PrincipalCatalogoServlet</servlet-name>
         <url-pattern>/catalogo/Principal</url-pattern>
         <url-pattern>/catalogo/</url-pattern>
     </servlet-mapping>

</web-app>